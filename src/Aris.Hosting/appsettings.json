{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ToolingOptions": {
    "ExtractionRoot": "",
    "Version": "0.1.0"
  },
  "WorkspaceOptions": {
    "DefaultWorkspacePath": ""
  },
  "Retoc": {
    "DefaultTimeoutSeconds": 300,
    "DefaultCompressionFormat": "Zlib",
    "DefaultCompressionLevel": 6,
    "AllowedAdditionalArgs": [
      "--verbose",
      "--no-warnings"
    ],
    "MaxLogBytes": 5242880,
    "StagingRoot": "",
    "EnableStructuredLogs": false
  }
}
